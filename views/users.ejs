<!DOCTYPE html>
<html>
<head>
  <title>Users List</title>
</head>
<body>
  <h2>Registered Users</h2>
  <table border="1" cellpadding="8" cellspacing="0">
    <thead>
      <tr>
        <th>#</th>
        <th>Name</th>
        <th>Unique ID</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach((user, index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= user.name %></td>
          <td><%= user.uniqueId %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>

  <p><a href="/">Back to home</a></p>
</body>
</html>
